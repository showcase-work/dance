<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>SEAT | DASHBOARD</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />


    <!-- Bootstrap core CSS     -->
    <link href="/dashboard/assets/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Animation library for notifications   -->
    <link href="/dashboard/assets/css/animate.min.css" rel="stylesheet"/>

    <!--  Light Bootstrap Table core CSS    -->
    <link href="/dashboard/assets/css/light-bootstrap-dashboard.css" rel="stylesheet"/>


    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="/dashboard/assets/css/pe-icon-7-stroke.css" rel="stylesheet" />

    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="/dashboard/assets/css/demo.css" rel="stylesheet" />

    <style type="text/css">
    .dashboard-approving-videos .team-details{
        padding:0px;
        margin:0px;
    }
    .dashboard-approving-videos .team-details li{
        display: block;
        text-align: left;
        margin:0px;
        padding:0px;
    }


    .navbar-left .active a{
        color:inherit!important;
    }
    </style>

</head>
<body>

<div class="wrapper">
    <div class="sidebar" data-color="grey" data-image="/dashboard/assets/img/dance.jpeg">
    
        <div class="sidebar-wrapper">
            <div class="logo">
                SEAT
            </div>

            <ul class="nav">
                <li class="">
                    <a href="/admin">
                        <i class="pe-7s-news-paper"></i>
                        <p>Dashboard</p>
                    </a>
                </li>
                <li class="active">
                    <a href="/admin/videos">
                        <i class="pe-7s-note2"></i>
                        <p>Manage Videos</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="pe-7s-graph"></i>
                        <p>Estad√≠sticas</p>
                    </a>
                </li>
            </ul>
        </div>
    </div>

    <div class="main-panel">
        <nav class="navbar navbar-default navbar-fixed">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="#">Manage Videos</a>
                </div>
                <div class="collapse navbar-collapse">
                    

                    <ul class="nav navbar-nav navbar-left">
                        <li class="
                            <%if(tab == "unevaluated"){%>
                                active
                            <%}%>
                        ">
                              <a href="/admin/videos">
                                    Un Evaluated
                              </a>
                        </li>
                        <li class="
                        <%if(tab == "accepted"){%>
                            active
                        <%}%>">
                              <a href="/admin/videos/accepted">
                                    Accepted
                              </a>
                        </li>
                        <li class="<%if(tab == "declined"){%>
                                active
                            <%}%>">
                              <a href="/admin/videos/declined">
                                    Declined
                              </a>
                        </li>
                        </li>
                        
                        <li class="separator hidden-lg hidden-md"></li>
                    </ul>

                </div>
            </div>
        </nav>

    <div class="main user-dashboard-main dashboard-approving-videos">
        <div class="content">
            <div class="container-fluid">
                <div class="row" style="padding:20px; text-align: center;">
                    <%
                        videos.forEach(function(team){%><div class="dashboard-videos-view" style="display: inline-block;vertical-align: width:40%; top; min-width: 300px; padding:5px; margin:10px; background-color: #ebebeb; padding-top: 0px;">
                                    <div class="">
                                        <!-- <video
                                          controls
                                          autoplay
                                          class="cld-video-player"
                                          data-cld-public-id="http://res.cloudinary.com/diaa7wkfp/video/upload/l_video:<%=team.public_id%>,w_300,g_east/<%=team.public_id%>.mp4"
                                          data-cld-autoplay-mode="false"
                                        >
                                        </video>  -->
                                        <!-- <video
                                          controls
                                          autoplay
                                          class="cld-video-player"
                                          data-cld-public-id="<%=team.public_id%>"
                                          data-cld-autoplay-mode="false"
                                        >
                                        </video>  -->
                                        <video width="320" height="240" controls>
                                          <source src="http://res.cloudinary.com/annaraujo/video/upload/<%=team.public_id%>.mp4" type="video/mp4">
                                          <source src="http://res.cloudinary.com/annaraujo/video/upload/<%=team.public_id%>.mp4" type="video/ogg">
                                          <source src="http://res.cloudinary.com/annaraujo/video/upload/<%=team.public_id%>.mp4" type="video/webm">
                                        Your browser does not support the video tag.
                                        </video>
                                    </div>
                                    <div class="below-video">
                                        <ul class="team-details">
                                            <li><%=team.teamName%></li>
                                            <li><%=team.city%>/<%=team.state%></li>
                                            <% if(tab == "unevaluated"){ %>
                                                <li style="margin-top:5px; "><button class="btn btn-primary approve" value="<%=team.id%>">Approve</button><button class="btn btn-danger decline" value="<%=team.id%>" style="float: right">Decline</button></li>
                                            <% } %>
                                            <% if(tab == "accepted"){ %>
                                                <li style="margin-top:5px; "><button class="btn btn-warning unevaluate" value="<%=team.id%>">Unevaluate</button><button class="btn btn-danger decline" value="<%=team.id%>" style="float: right">Decline</button></li>
                                            <% } %>
                                            <% if(tab == "declined"){ %>
                                                <li style="margin-top:5px; "><button class="btn btn-primary approve" value="<%=team.id%>">Approve</button><button class="btn btn-warning unevaluate" value="<%=team.id%>" style="float: right">Unevaluate</button></li>
                                            <% } %>
                                        </ul>
                                        <!-- <button class="voting-button"><i class="fa fa-facebook"></i> VOTAR</button> -->
                                        <div class="clearboth"></div>
                                    </div>
                                </div>
                    <%})%>
                </div>
            </div>

        </div>        
    </div>

    </div>
</div>

</body>
<!--   Core JS Files   -->
<script src="/dashboard/assets/js/jquery-1.10.2.js" type="text/javascript"></script>
<script src="/dashboard/assets/js/bootstrap.min.js" type="text/javascript"></script>

<!--  Checkbox, Radio & Switch Plugins -->
<script src="/dashboard/assets/js/bootstrap-checkbox-radio-switch.js"></script>

<!--  Charts Plugin -->
<script src="/dashboard/assets/js/chartist.min.js"></script>

<!--  Notifications Plugin    -->
<script src="/dashboard/assets/js/bootstrap-notify.js"></script>

<!--  Google Maps Plugin    -->
<script type="/dashboard/text/javascript" src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>

<!-- Light Bootstrap Table Core javascript and methods for Demo purpose -->
<script src="/dashboard/assets/js/light-bootstrap-dashboard.js"></script>

<script type="text/javascript">
    $(".approve").click(function(){
        var self = $(this);
        var id = self.val();
        console.log(self.val());
        $.ajax({
            url:"/admin/videos/update-status",
            method:"post",
            data:{
                status:"Accepted",
                id:id
            },
            success:function(data){
                console.log("worked well");
                self.parent().parent().parent().parent().fadeOut();
            },
            error:function(err){
                console.log(err);
            }
        })
    })

    $(".decline").click(function(){
        var self = $(this);
        var id = self.val();
        console.log(self.val());
        $.ajax({
            url:"/admin/videos/update-status",
            method:"post",
            data:{
                status:"Declined",
                id:id
            },
            success:function(data){
                console.log("worked well");
                self.parent().parent().parent().parent().fadeOut();
            },
            error:function(err){
                console.log(err);
            }
        })
    })

    $(".unevaluate").click(function(){
        var self = $(this);
        var id = self.val();
        console.log(self.val());
        $.ajax({
            url:"/admin/videos/update-status",
            method:"post",
            data:{
                status:"En evaluaci√≥n",
                id:id
            },
            success:function(data){
                self.parent().parent().parent().parent().fadeOut();
            },
            error:function(err){
                console.log(err);
            }
        })
    })

</script>

</html>
