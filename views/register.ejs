    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/uikit/2.27.2/js/components/slideshow.js"></script>

    <script type="text/javascript">
        $(document).ready(function(){

            $("form").submit(function(e){
                e.preventDefault();
                return false;
            })
            var x = 1;
            $(".child-container .continue").click(function(e){
                x++;
                if(x<7){
                    $(".dot"+x).addClass("active");
                }
                console.log("ajdkf");
                e.preventDefault();
                var self = $(this);
                self.parent().parent().removeClass("active");
                self.parent().parent().next().addClass("active");
                return false;
                console.log("working in here");
            })


        })
        
    </script>
</body>
